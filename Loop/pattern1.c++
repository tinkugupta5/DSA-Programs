#include<iostream>
using namespace std;

int main() {
    int n = 4 ;
   
    int i = 1;

    while (i<=n)
    {
        /* code */
        int j=1; 
        while (j<=n)
        {
            /* code */
            cout<<"*";
            j = j+1;
        }
        cout<<endl;
        i=i+1;
        
    }

    // cout<<"hellow"<<endl;
    

}